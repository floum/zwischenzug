<h2>Games</h2>

<%= link_to 'New Game', new_game_path %>

<table>
  <thead>
    <tr>
      <td>Players</td>
      <td>Result</td>
      <td></td>
    </tr>
  </thead>
  <tbody>
<% @games.each do |game| %>
  <tr>
  <td><%= "#{game.white} - #{game.black}" %></td>
  <td><%= game.result %></td>
  <td>Create Review</td>
<% end %>
  </tbody>
